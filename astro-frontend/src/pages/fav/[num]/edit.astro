---
import Layout from "../../../layouts/Layout.astro";
import EditFavorite from "../../../components/EditFavorite";

export function getStaticPaths() {
  const paths = [];
  for (let num = 0; num < 10; num++) {
    paths.push({ params: { num } });
  }
  return paths;
}

const { num } = Astro.params;
---

<Layout title="Edit favorite">
  <EditFavorite client:load num={Number.parseInt(num.toString())} />
</Layout>
