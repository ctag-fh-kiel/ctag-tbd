---
import Layout from "../../layouts/Layout.astro";
import PluginParams from "../../components/PluginParams";

export function getStaticPaths() {
  return [{ params: { channel: "0" } }, { params: { channel: "1" } }];
}

const { channel } = Astro.params;
---

<Layout title={`Channel ${channel}`}>
  <PluginParams client:load channel={channel} />
</Layout>
