---
import Layout from "../../layouts/Layout.astro";
import LoadChannelPreset from "../../components/LoadChannelPreset";
import type { Channel } from "../../stores/pluginsStore";

export function getStaticPaths() {
  return [{ params: { channel: "0" } }, { params: { channel: "1" } }];
}

const { channel } = Astro.params;
---

<Layout title={`Channel ${channel} preset`}>
  <LoadChannelPreset client:load channel={channel as Channel} />
</Layout>
